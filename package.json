{
  "name": "TAG",
  "version": "1.0.0",
  "description": "A text annotation engine",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "appId": "Tag",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./tag.ico",
      "extraResources": "py"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createStartMenuShortcut": true,
      "createDesktopShortcut": true
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.1",
    "spectron": "^9.0.0"
  },
  "dependencies": {
    "electron-packager": "^14.1.0",
    "jquery": "^3.4.1",
    "path": "^0.12.7",
    "python-shell": "^1.0.8"
  }
}
